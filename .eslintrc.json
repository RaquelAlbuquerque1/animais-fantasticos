{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions":0,
        "no-param-reassign":0,
        "no-named-as-default":0,
        "no-console":0,
        "arrow-parens":0,
        "max-len":0,
        "max-classes-per-file":0
    }
}
